<%= form_with model: registration do |f| %>
  <%= f.hidden_field :instructor_id, value: current_user.id %>
  <div class="mb-2">
    <label class="form-label"> Name </label>
    <div>
      <%= f.text_field :name, class: "form-control" %>
    </div>
  </div>
  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>