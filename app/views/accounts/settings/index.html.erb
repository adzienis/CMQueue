<h1>
  Settings
</h1>

<hr>
<div class="card">
  <div class="card-body">
    <ul class="list-group">
      <%= render(Users::Settings::DesktopNotificationComponent.new(setting: current_user
        .settings
        .with_key("desktop_notifications")
        .first,
                                                                   record: current_user, options: {
                                                                     path: account_setting_path(current_user
                                                                                                  .settings
                                                                                                  .with_key("desktop_notifications")
                                                                                                  .first)
                                                                   }))
      %>
    </ul>
  </div>
</div>
